<launch>
	<rosparam command="load" file="$(find interface_joy)/param/interface_params.yaml" />

	<node
		pkg="interface_joy"
		type="interface_joy_node.py"
		name="interface_joy"
		output="screen"
	>
	</node>
	<node respawn="true" pkg="joy" type="joy_node" name="joy_puma" >
        <param name="dev" type="string" value="/dev/input/js0" />
        <param name="deadzone" value="0.12" />
    </node>

</launch>