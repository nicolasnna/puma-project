    <!-- <include file="$(find robot_localization)/launch/d455_loc.launch"/> -->

    <include file="$(find rtabmap_ros)/launch/rtabmap.launch">
        <arg name="args" value="--delete_db_on_start"/>
        <arg name="odom_topic" value="/puma/odometry/filtered"/>
        <arg name="rgb_topic" value="/puma/camera/color/image_raw"/>
        <arg name="camera_info_topic" value="/puma/camera/color/camera_info"/>
        <arg name="depth_topic" value="/puma/camera/aligned_depth_to_color/image_rect_raw"/>
        <arg name="depth_camera_info_topic" value="/puma/camera/aligned_depth_to_color/camera_info"/>
        <arg name="rtabmapviz" value="false"/>
        <arg name="rviz" value="true"/>
        <arg name="odom_frame_id" value="odom"/>
        <arg name="publish_tf_odom" value="true"/>
        <arg name="publish_tf_map" value="true"/>      
        <arg name="map_frame_id" value="map"/>        
        <arg name="visual_odometry" value="true"/>
        <arg name="scan_cloud_assembling_noise_radius"   default="0.01"/>   
        <arg name="scan_cloud_assembling_noise_min_neighbors"   default="2000"/>
        
    </include>

