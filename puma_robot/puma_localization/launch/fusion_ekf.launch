<?xml version="1.0"?>
<launch>
  <rosparam command="load" file="$(find puma_localization)/config/fusion_ekf_params.yaml"/>

  <node name="ekf_odom" pkg="robot_localization" type="ekf_localization_node" clear_params="true"/>
  
  <!-- Spawn the robot -->
  <param name="robot_description" command="$(find xacro)/xacro '$(find puma_model)/urdf/puma_model.urdf.xacro'" />

  <!-- convert joint states to TF transforms for rviz, etc -->
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="joint_state_publisher"
    pkg="joint_state_publisher"
    type="joint_state_publisher" />
  <!-- <node name="ekf_map" pkg="robot_localization" type="ekf_localization_node"  clear_params="true">
    <remap from="odometry/filtered" to="odometry/filtered_map"/>
  </node> -->
  
</launch>